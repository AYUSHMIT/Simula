config_version=4

_global_script_classes=[  ]
_global_script_class_icons={

}

[application]

config/name="Simula"
run/main_scene="res://addons/godot-haskell-plugin/demo/Demo.tscn"

[display]

window/vsync/use_vsync=false

[gdnative]

singletons=[ "res://addons/godot-openhmd/godot_openhmd.gdnlib", "res://addons/godot-openvr/godot_openvr.gdnlib" ]

[physics]

common/physics_fps=90
2d/thread_model=2

[rendering]

quality/filters/anisotropic_filter_level=16
quality/filters/msaa=3
environment/default_environment="res://default_env.tres"
